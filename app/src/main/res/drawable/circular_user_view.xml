<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape
            android:shape="oval">

            <gradient
                android:angle="90"
                android:endColor="#A357A701"
                android:startColor="#B53C7301" />

            <stroke
                android:width="2dp"
                android:color="@color/secondaryColor" />

            <size
                android:width="40dp"
                android:height="40dp" />
            <corners android:radius="18dp" />
        </shape>
    </item>
    <item android:drawable="@drawable/ic_baseline_face_16" />
</layer-list>